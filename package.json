    "prepare": "husky install",
    "clear-dts": "find ./src -name '*.d.ts' -delete",
    "test-a11y": "./node_modules/.bin/tsc --project tests/tsconfig.a11y.json && node tests/test.js",
    "serve-docs": "./node_modules/.bin/ws -p 3000 -d docs"
  },
  "files": [
    "src/**/*",
    "lib/**/*",
    "index.d.ts"
